{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended", "plugin:import/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "linebreak-style": 0,
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "prefer-destructuring": 0,
    "arrow-body-style": 0,
    "comma-dangle": 0,
    // "prettier/prettier": "error",
    "react/prop-types": 0
  },
  "plugins": ["react", "prettier", "import"]
}
